{
  "name": "c10ck",
  "version": "1.0.0",
  "description": "neon desktop clock",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . c10ck --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"c10ck\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "Mok Xiao Fan",
  "license": "ISC",
  "dependencies": {
    "auto-launch": "^5.0.5",
    "braces": "^3.0.2",
    "decompress-zip": "^0.3.3",
    "dot-prop": "^6.0.1",
    "electron": ">=9.4.0",
    "electron-packager": "^12.1.0",
    "electron-reload": "^1.2.3",
    "electron-store": "^2.0.0",
    "extend": "^3.0.2",
    "ini": "^2.0.0",
    "minimist": "^1.2.5",
    "xmldom": ">=0.5.0",
    "yargs-parser": "^20.2.7"
  }
}
